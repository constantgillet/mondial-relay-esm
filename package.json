  "keywords": [
    "mondialrelay",
    "mondial-relay"
  ],
  "peerDependencies": {
    "react": "^18.3.1",
    "soap": "^1.1.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.2.1",
    "@types/jquery": "^3.5.30",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "chalk": "^4.1.2",
    "eslint": "8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.3",
    "xml2js": "^0.6.2",
    "zod": "^3.23.8"
  }
